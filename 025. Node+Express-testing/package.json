    "chai": "^4.3.6",
    "cheerio": "^1.0.0-rc.10",
    "cross-env": "^7.0.3",
    "got": "^12.0.1",
    "mocha": "^9.2.2",
    "nodemon": "^2.0.15"
  },
  "dependencies": {
    "body-parser": "^1.19.2",
    "express": "^4.17.3",
